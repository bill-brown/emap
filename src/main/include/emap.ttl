@prefix : <http://www.colorfulsoftware.com/emap/lv2#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix work: <http://lv2plug.in/ns/ext/worker#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix ui: <http://lv2plug.in/ns/extensions/ui#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .


:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://www.colorfulsoftware.com/emap/lv2ui>
    a ui:GtkUI ;
    ui:binary <libemap-0.0.2-SNAPSHOT.so>;
    lv2:requiredFeature urid:map ;
        ui:portNotification [
                ui:plugin <http://lv2plug.in/plugins/eg-sampler> ;
                lv2:symbol "notify" ;
                ui:notifyType atom:Blank
        ] .

<http://www.colorfulsoftware.com/emap/lv2> a lv2:Plugin;
  a lv2:InstrumentPlugin;
  doap:name "EMAP";
  doap:maintainer [ foaf:name "Bill Brown" ; ] ;
  doap:license <http://usefulinc.com/doap/licenses/apache2> ;
  lv2:requiredFeature urid:map ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:optionalFeature <http://lv2plug.in/ns/ext/instance-access> ;
  lv2:optionalFeature <http://lv2plug.in/ns/ext/data-access> ;
  lv2:extensionData state:interface, work:interface;
  ui:ui <http://www.colorfulsoftware.com/emap/lv2ui>;
  lv2:optionalFeature epp:supportsStrictBounds ;
  lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;
  pg:mainOutput :out ;
  lv2:port 
  
  [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "out_Left" ;
        lv2:name "out_Left" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "out_Right" ;
        lv2:name "out_Right" ;
        lv2:designation pg:right ;
        pg:group :out ;
    ],
    [
        a lv2:InputPort ,
        		atom:AtomPort ;
        atom:bufferType atom:Sequence ;
    	atom:supports midi:MidiEvent; #, patch:Message ;		
        lv2:designation lv2:control ;
        lv2:index 2 ;
        lv2:symbol "midi_in" ;
        lv2:name "midi_in" ;
    ],
    [
		a lv2:OutputPort ,
        	atom:AtomPort ;
        atom:bufferType atom:Sequence ;
		lv2:index 3 ;
		lv2:symbol "notify" ;
		lv2:name "notify" ;
    ].
   
    
